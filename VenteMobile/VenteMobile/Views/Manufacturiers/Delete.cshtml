@model VenteMobile.Models.Manufacturier

@{
    ViewData["Title"] = "Delete";
}

<h3 style="text-align:center"><B>Supprimer Manufacturier</B></h3>
<h3>Voulez vous vraiment le supprimer?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.ManufacturierMarque)*@
            Marque
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ManufacturierMarque)
        </dd>
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.ManufacturierSiteWeb)*@
            SiteWeb
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ManufacturierSiteWeb)
        </dd>
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.ManufacturierOrigine)*@
            Siège social
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ManufacturierOrigine)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="ManufacturierId" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste</a>
    </form>
</div>
