@model VenteMobile.Models.Vendeur

@{
    ViewData["Title"] = "Delete";
}

    <h6>Supprimer Vendeur</h6>

    <h3>Voulez vous vraiment le supprimer?</h3>
<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.VendeurNom)*@
            Nom
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VendeurNom)
        </dd>
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.VendeurAddress)*@
            Address
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VendeurAddress)
        </dd>
        <dt class="col-sm-2">
            @*@Html.DisplayNameFor(model => model.VendeurNoTel)*@
            NoTel
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VendeurNoTel)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="VendeurId" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste</a>
    </form>
</div>
